{
  "name": "nuovi-media/laravel-letterboxd",
  "description": "Letterboxd API client for Laravel",
  "type": "library",
  "require": {
    "illuminate/support": "^8.51",
    "php": "^8.0",
    "illuminate/http": "^8.51",
    "ext-json": "*"
  },
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Lorenzo Breda",
      "email": "lorenzo@lbreda.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "NuoviMedia\\LetterboxdClient\\LetterboxdServiceProvider"
      ],
      "aliases": {
        "Config": "Illuminate\\Support\\Facades\\Config",
        "Http": "Illuminate\\Support\\Facades\\Http"
      }
    }
  },
  "autoload": {
    "classmap": [
      "src"
    ],
    "psr-4": {
      "NuoviMedia\\LetterboxdClient\\": "src/"
    }
  }
}
